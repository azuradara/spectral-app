"generated":{"line":263,"column":36}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":77,"column":2},"generated":{"line":263,"column":43}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":65,"column":57},"generated":{"line":269,"column":5}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":65,"column":57},"generated":{"line":269,"column":6}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":78,"column":1},"generated":{"line":270,"column":0}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":65,"column":32},"generated":{"line":270,"column":3}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":78,"column":1},"generated":{"line":271,"column":0}},{"source":"store/deeds/auth.ts","name":null,"original":{"line":65,"column":7},"generated":{"line":271,"column":1}}],"sources":{"store/deeds/auth.ts":"import axios from 'axios';\r\nimport { Dispatch } from 'redux';\r\n\r\nimport { DeedTypes } from '.';\r\nimport { CreateNotificationDeed } from '.';\r\nimport { ApiResponse, LoginData, User } from '../../lib/interfaces';\r\n\r\n// LOGIN USER\r\n\r\nexport interface LoginUserDeed<T> {\r\n  type:\r\n    | DeedTypes.loginUser\r\n    | DeedTypes.loginUserError\r\n    | DeedTypes.loginUserSuccess;\r\n  payload: T;\r\n}\r\n\r\nexport const loginUser =\r\n  (formData: LoginData) => async (dispatch: Dispatch) => {\r\n    dispatch<LoginUserDeed<undefined>>({\r\n      type: DeedTypes.loginUser,\r\n      payload: undefined,\r\n    });\r\n\r\n    try {\r\n      const res = await axios.post<ApiResponse<User>>('/auth/login', formData);\r\n\r\n      if (res.data.error) {\r\n        console.log('err');\r\n        return dispatch<CreateNotificationDeed>({\r\n          type: DeedTypes.createNotification,\r\n          payload: {\r\n            title: 'error',\r\n            message: 'Invalid Credentials',\r\n            type: 'error',\r\n          },\r\n        });\r\n      }\r\n\r\n      dispatch<CreateNotificationDeed>({\r\n        type: DeedTypes.createNotification,\r\n        payload: {\r\n          title: 'success',\r\n          message: `User ${res.data.data.user.username} logged in.`,\r\n          type: 'default',\r\n        },\r\n      });\r\n\r\n      dispatch<LoginUserDeed<User>>({\r\n        type: DeedTypes.loginUserSuccess,\r\n        payload: res.data.data,\r\n      });\r\n\r\n      localStorage.setItem('token', res.data.data.token);\r\n    } catch (err) {\r\n      console.log(err);\r\n    }\r\n  };\r\n\r\nexport interface LogoutUserDeed {\r\n  type: DeedTypes.logoutUser;\r\n  payload: null;\r\n}\r\n\r\nexport const logoutUser = () => async (dispatch: Dispatch) => {\r\n  dispatch<LogoutUserDeed>({\r\n    type: DeedTypes.logoutUser,\r\n    payload: null,\r\n  });\r\n\r\n  try {\r\n    await axios.post<ApiResponse<string>>('/auth/logout');\r\n  } catch (err) {\r\n    console.log(err);\r\n  }\r\n\r\n  localStorage.removeItem('token');\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"ce80f9f14c650bdc1bf99f162e93e1bf","cacheData":{"env":{}}}